apiVersion = apps/v1
kind = Deployment

metadata.name = ^NAME
metadata.labels.app = nginx

spec = {
  replicas = :1
  selector.matchLabels.app = nginx
  
  template.metadata.labels.app = nginx
  template.spec.containers = [
      {
        name = nginx
        image = "nginx:1.7.9"
        ports = [ containerPort = :80 ]
      }
  ]
}
